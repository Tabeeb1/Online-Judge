#include <bits/stdc++.h>
using namespace std;

int main() {
    int n,h;
    int width = 0;
    cin >> n >> h;
    int arr[n];
    for(int i = 0;i<n;i++){
        int people;
        cin >> people;
        arr[i] = people;
    }
    for(int i = 0;i<n;i++){
        if(arr[i] <= h){
            width += 1;
        }
        else{
            width += 2;
        }
    }
    cout << width;
}
